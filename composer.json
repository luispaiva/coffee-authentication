  "description": "Um plugin criado para autenticação JWT no WordPress.",
  "authors": [
    {
      "name": "Luis Paiva",
      "email": "contato@luispaiva.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require": {
